srcdir = @srcdir@
top_srcdir = @top_srcdir@
include ../config.make

SRCS = ttymsg.c version.c getopt.c getopt1.c stub_tgetent.c \
	localhost.c daemon.c strerror.c hstrerror.c strdup.c logout.c \
	logwtmp.c logwtmpko.c cleansess.c openpty.c

DISTFILES = $(SRCS)

OBJS = ttymsg.o version.o localhost.o cleansess.o logwtmpko.o @LIBOBJS@
CLEAN = libinetutils.a $(OBJS)

CPPFLAGS += $(PATHDEF_DEVNULL) $(PATHDEF_TTY_PFX) \
	$(PATHDEF_WTMP) $(PATHDEF_UTMP)

all: libinetutils.a
libinetutils.a: $(OBJS)
	$(AR) rcv $@ $?
	$(RANLIB) $@

include $(RULES)
